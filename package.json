{
  "name": "karzok",
  "repository": "https://git.sr.ht/~kogeletey/karzok",
  "author": "kogeletey",
  "private": true,
  "scripts": {
    "postcss": "postcss ./public/*.css --replace",
    "compile:js": "swc helpers --only -d static",
    "link": "rm -rf static && mkdir static && ln -s ../node_modules/@fontsource ../node_modules/katex ../node_modules/normalize.css ../assets/ static/",
    "clean": "rm -rf static node_modules",
    "gen": "npm run link && npm run compile:js && zola build && npm run postcss"
  },
  "dependencies": {
    "@fontsource/inter": "^4.5.0",
    "@fontsource/jetbrains-mono": "^4.5.0",
    "katex": "^0.13.13",
    "normalize.css": "^8.0.1"
  },
  "otherDependencies": {
    "zola": "0.14.0"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.53",
    "@swc/core": "^1.2.119",
    "autoprefixer": "^10.4.0",
    "postcss": "^8.4.4",
    "postcss-cli": "^9.1.0"
  }
}

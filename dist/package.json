{
  "name": "karzok",
  "repository": "https://git.sr.ht/~kogeletey/karzok",
  "version": "0.2.9",
  "author": "kogeletey",
  "scripts": {
    "link": "cp -r node_modules/@fontsource node_modules/katex node_modules/modern-normalize node_modules/mermaid static",
    "build": "zola-bin build",
    "build:local": "npm run link && zola-bin build",
    "watch": "zola-bin serve -O -f"
  },
  "files": [
    "static/*",
    "templates/**/*",
    "theme.toml",
    "package.json"
  ],
  "dependencies": {
    "@fontsource/inter": "^4.5.15",
    "@fontsource/jetbrains-mono": "^4.5.12",
    "katex": "^0.16.4",
    "mermaid": "^9.3.0",
    "modern-normalize": "^1.1.0"
  },
  "devDependencies": {
    "zola-bin": "^0.3.5"
  }
}
